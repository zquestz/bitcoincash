<!doctype html>
<html lang="<%= I18n.locale.to_s.split('-').first %>">
  <head>
    <meta charset="utf-8">
    <title><%= I18n.t(:'title.graphics').strip %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="<%= I18n.t(:'meta.graphics.description').strip %>">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= I18n.t(:'meta.graphics.twitter.title').strip %>">
    <meta name="twitter:description" content="<%= I18n.t(:'meta.graphics.twitter.description').strip %>">
    <meta name="twitter:image" content="https://www.bitcoincash.org/img/bitcoincash-twitter.webp">
    <meta property="og:title" content="<%= I18n.t(:'meta.graphics.og.title').strip %>">
    <meta property="og:description" content="<%= I18n.t(:'meta.graphics.og.description').strip %>">
    <meta property="og:image" content="https://www.bitcoincash.org/img/bitcoincash-twitter.webp">
    <%- if ['ar', 'fa'].include?(I18n.locale.to_s.split('-').first) %>
      <style>
        section { direction: rtl; }
      </style>
    <%- end %>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="/css/deps.min.css?v=<%= Digest::SHA256.file 'html/css/deps.min.css' %>" rel="stylesheet" type="text/css" media="all">
    <link href="/css/custom.min.css?v=<%= Digest::SHA256.file 'html/css/custom.min.css' %>" rel="stylesheet" type="text/css" media="all">
    <link href="https://fonts.googleapis.com/css2?family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap" rel="stylesheet" type="text/css" media="all">
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-17656782-2"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments)};
      gtag('js', new Date());

      gtag('config', 'UA-17656782-2');
    </script>
  </head>
  <body>
    <a href="#" id="start"></a>
    <header id="fh5co-header">
      <div class="container">
        <nav class="navbar navbar-default">
          <div class="navbar-header">
            <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"><i></i></a>
            <a class="navbar-brand" href="../">
              <img loading="lazy" src="/img/bitcoin-cash-logo-horizontal.svg" alt="Bitcoin Cash" class="darkit">
              <img loading="lazy" src="/img/bitcoin-cash-logo-horizontal-wt.svg" alt="Bitcoin Cash" class="lightit">
            </a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="active"><a href="../" class="external" data-nav-section="home"><span><%= I18n.t(:'graphics.sections.home').strip %></span></a></li>
              <li><a href="#" data-nav-section="logos"><span><%= I18n.t(:'graphics.sections.logos').strip %></span></a></li>
              <li><a href="#" data-nav-section="merchant"><span><%= I18n.t(:'graphics.sections.merchant').strip %></span></a></li>
              <li><a href="#" data-nav-section="identity"><span><%= I18n.t(:'graphics.sections.identity').strip %></span></a></li>
            </ul>
          </div>
        </nav>
      </div>
    </header>
    <section id="fh5co-home" data-section="home" class="site-header">
      <div class="container">
        <div class="text-wrap">
          <div class="text-inner">
            <h1><%= I18n.t(:'graphics.title').strip %></h1>
            <h2><%= I18n.t(:'graphics.description').strip %></h2>
          </div>
        </div>
      </div>
      <div class="gap"></div>
    </section>
    <div class="main-container" id="graphics-section">
      <section id="fh5co-logos" data-section="logos">
        <div class="container">
          <div class="row">
            <div class="col-md-12 section-heading text-center">
              <h2><%= I18n.t(:'graphics.logos').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext">
                  <h3><%= I18n.t(:'graphics.download_png_svg').strip %></h3>
                  <h4><a href="/img/Bitcoin-Cash-Logos.zip" target="_blank"><%= I18n.t(:'graphics.download_all').strip %></a></h4>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-4">
              <div class="fh5co-person text-center">
                <h6><%= I18n.t(:'graphics.types.primary').strip %></h6>
                <img loading="lazy" src="/img/green/bitcoin-cash-logo-horizontal.svg" alt="Bitcoin Cash Logo">
                <a href="/img/green/bitcoin-cash-logo-horizontal.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-logo-horizontal.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center">
                <h6><%= I18n.t(:'graphics.types.white').strip %></h6>
                <img loading="lazy" src="/img/green/bitcoin-cash-logo-horizontal-wt.svg" alt="Bitcoin Cash Logo" class="dark">
                <a href="/img/green/bitcoin-cash-logo-horizontal-wt.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-logo-horizontal-wt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center">
                <h6><%= I18n.t(:'graphics.types.white').strip %></h6>
                <img loading="lazy" src="/img/green/bitcoin-cash-logo-horizontal-fullwt.svg" alt="Bitcoin Cash Logo" class="dark">
                <a href="/img/green/bitcoin-cash-logo-horizontal-fullwt.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-logo-horizontal-fullwt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center">
                <h6><%= I18n.t(:'graphics.types.mark').strip %></h6>
                <img loading="lazy" src="/img/green/bitcoin-cash-circle.svg" alt="Bitcoin Cash Logo" class="flag">
                <a href="/img/green/bitcoin-cash-circle.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-circle.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
          </div>
        </div>
      </section>
      <section id="fh5co-merchant" data-section="merchant">
        <div class="container">
          <div class="row">
            <div class="col-md-12 section-heading text-center">
              <h2><%= I18n.t(:'graphics.merchant.title').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext">
                  <h3><%= I18n.t(:'graphics.merchant.description').strip %></h3>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div class="fh5co-person text-center">
                <img loading="lazy" src="/img/green/bitcoin-cash-accepted-here-gt.svg" alt="Bitcoin Cash Accepted Here">
                <a href="/img/green/bitcoin-cash-accepted-here-gt.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-accepted-here-gt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-6">
              <div class="fh5co-person text-center">
                <img loading="lazy" src="/img/green/bitcoin-cash-accepted-here-wt.svg" alt="Bitcoin Cash Accepted Here">
                <a href="/img/green/bitcoin-cash-accepted-here-wt.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-accepted-here-wt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-6">
              <div class="fh5co-person text-center">
                <img loading="lazy" src="/img/green/bitcoin-cash-accepted-here-frame.svg" alt="Bitcoin Cash Accepted Here">
                <a href="/img/green/bitcoin-cash-accepted-here-frame.webp" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/img/green/bitcoin-cash-accepted-here-frame.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
          </div>
        </div>
      </section>
      <section id="fh5co-identity" data-section="identity">
        <div class="container">
          <div class="row">
            <div class="col-md-12 section-heading text-center">
              <h2><%= I18n.t(:'graphics.identity.title').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext">
                  <h3><%= I18n.t(:'graphics.identity.description').strip %></h3>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div class="font">
                <h3><%= I18n.t(:'graphics.identity.font').strip %></h3>
                <h4 class="font-color-sample" style="font-weight:bold">ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890?!%</h4>
                <a href="https://fonts.google.com/specimen/Ubuntu?selection.family=Ubuntu:700" class="btn btn-primary" target="_blank"><%= I18n.t(:'graphics.identity.download').strip %></a>
              </div>
              <div class="font">
                <h3><%= I18n.t(:'graphics.identity.font_italic').strip %></h3>
                <h4 class="font-color-sample" style="font-style:italic">ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890?!%</h4>
                <a href="https://fonts.google.com/specimen/Ubuntu?selection.family=Ubuntu:700i" class="btn btn-primary" target="_blank"><%= I18n.t(:'graphics.identity.download').strip %></a>
              </div>
            </div>
            <div class="col-md-6">
              <div class="color row">
                <div class="col-md-3"><span class="box-color" style="background:#0AC18E"></span></div>
                <div class="col-md-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #0AC18E<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 10 / 193 / 142<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 95 / 0 / 26 / 24<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: 3395 C
                </div>
              </div>
              <div class="color row">
                <div class="col-md-3"><span class="box-color" style="background:#4D4D4D"></span></div>
                <div class="col-md-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #4D4D4D<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 77 / 77 / 77<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 65 / 58 / 57 / 37<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: 11C
                </div>
              </div>
              <div class="color row">
                <div class="col-md-3"><span class="box-color" style="background:#FFFFFF"></span></div>
                <div class="col-md-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #FFFFFF<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 255 / 255 / 255<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 0 / 0 / 0 / 0<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: paper
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <footer class="space--xs footer-1">
        <div class="container">
          <div class="row">
            <div class="col-sm-6">
              <span class="type--fine-print">&copy;<span class="update-year"></span> bitcoincash.org</span>
            </div>
            <div class="col-sm-6 text-right text-center-xs">
              <ul class="social-list list-inline list--hover">
                <li>
                  <a href="https://www.reddit.com/r/Bitcoincash/" target="_blank">
                    <i class="socicon socicon-reddit icon icon--xs"></i>
                  </a>
                </li>
                <li>
                  <a href="https://github.com/zquestz/bitcoincash" target="_blank">
                    <i class="socicon socicon-github icon icon--xs"></i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <a class="back-to-top inner-link" href="#start" data-scroll-class="100vh:active">
      <i class="stack-interface stack-up-open-big"></i>
    </a>
    <script src="/js/modernizr-2.6.2.min.js"></script>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/jquery.easing.1.3.min.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/jquery.waypoints.min.js"></script>
    <script src="/js/smooth-scroll.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/scripts.min.js?v=<%= Digest::SHA256.file 'html/js/scripts.min.js' %>"></script>
  </body>
</html>
