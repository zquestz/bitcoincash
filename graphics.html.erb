<!doctype html>
<html lang="<%= I18n.locale.to_s.split('-').first %>">
  <head>
    <meta charset="utf-8">
    <title><%= I18n.t(:'title.graphics').strip %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="<%= I18n.t(:'meta.graphics.description').strip %>">
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= I18n.t(:'meta.graphics.twitter.title').strip %>">
    <meta name="twitter:description" content="<%= I18n.t(:'meta.graphics.twitter.description').strip %>">
    <meta name="twitter:image" content="https://www.bitcoincash.org/img/bitcoincash-twitter.png">
    <meta property="og:title" content="<%= I18n.t(:'meta.graphics.og.title').strip %>">
    <meta property="og:description" content="<%= I18n.t(:'meta.graphics.og.description').strip %>">
    <meta property="og:image" content="https://www.bitcoincash.org/img/bitcoincash.png">
    <link href="/css/stack-interface.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/css/socicon.css" rel="stylesheet" type="text/css" media="all" />
    <!-- <link href="/css/theme.css" rel="stylesheet" type="text/css" media="all" /> -->
    <link href="/css/lang.css" rel="stylesheet" type="text/css" media="all" />
    <link href="/css/custom.css?v=1" rel="stylesheet" type="text/css" media="all" />
    <link href="/css/bootstrap-graphics.css" rel="stylesheet" type="text/css" media="all" />
    <!-- <link rel="stylesheet" href="/css/animate.css"> -->
    <link rel="stylesheet" href="/css/icomoon.css">
    <link rel="stylesheet" href="/css/graphics.css">
    <link href='https://fonts.googleapis.com/css?family=Ubuntu:300,400,700' rel='stylesheet' type='text/css'>
    <script src="/js/modernizr-2.6.2.min.js"></script>
    <!-- Global Site Tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-17656782-2"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments)};
      gtag('js', new Date());
      gtag('config', 'UA-17656782-2');
    </script>
  </head>
  <body class=" ">
    <a id="start"></a>
    <header id="fh5co-header">
      <div class="container">
        <nav class="navbar navbar-default">
          <div class="navbar-header">
            <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"><i></i></a>
            <a class="navbar-brand" href="../">
              <img src="/graphics/img/bitcoin-cash-logo-horizontal-wt.svg" alt="Bitcoin Cash" class="lightit">
              <img src="/graphics/img/bitcoin-cash-logo-horizontal.svg" alt="Bitcoin Cash" class="darkit">
            </a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav navbar-right">
              <li class="active"><a href="../" class="external" data-nav-section="home"><span><%= I18n.t(:'graphics.sections.home').strip %></span></a></li>
              <li><a href="#" data-nav-section="logos"><span><%= I18n.t(:'graphics.sections.logos').strip %></span></a></li>
              <li><a href="#" data-nav-section="merchant"><span><%= I18n.t(:'graphics.sections.merchant').strip %></span></a></li>
              <li><a href="#" data-nav-section="identity"><span><%= I18n.t(:'graphics.sections.identity').strip %></span></a></li>
            </ul>
          </div>
        </nav>
      </div>
    </header>
    <section id="fh5co-home" data-section="home" class="site-header">
      <div class="container">
        <div class="text-wrap">
          <div class="text-inner">
            <div class="row">
              <div class="col-md-8 col-md-offset-2">
                <h1 class="to-animate"><%= I18n.t(:'graphics.title').strip %></h1>
                <h2 class="to-animate"><%= I18n.t(:'graphics.description').strip %></h2>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="slant"></div>
    </section>
    <div class="main-container" id="graphics-section">
      <section id="fh5co-logos" data-section="logos">
        <div class="container">
          <div class="row">
            <div class="col-md-12 section-heading text-center">
              <h2 class="to-animate"><%= I18n.t(:'graphics.logos').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext to-animate">
                  <h3><%= I18n.t(:'graphics.download_png_svg').strip %></h3>
                  <h4><a href="/graphics/img/Bitcoin-Cash-Logos.zip" target="_blank"><i class="icon-download"></i> <%= I18n.t(:'graphics.download_all').strip %></a></h4>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-4">
              <div class="fh5co-person text-center to-animate">
                <h6><%= I18n.t(:'graphics.types.primary').strip %></h6>
                <img src="/graphics/img/green/bitcoin-cash-logo-horizontal.svg" alt="Bitcoin Cash Logo">
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center to-animate">
                <h6><%= I18n.t(:'graphics.types.white').strip %></h6>
                <img src="/graphics/img/green/bitcoin-cash-logo-horizontal-wt.svg" alt="Bitcoin Cash Logo" class=" dark">
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal-wt.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal-wt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center to-animate">
                <h6><%= I18n.t(:'graphics.types.white').strip %></h6>
                <img src="/graphics/img/green/bitcoin-cash-logo-horizontal-fullwt.svg" alt="Bitcoin Cash Logo" class=" dark">
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal-fullwt.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-logo-horizontal-fullwt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-4">
              <div class="fh5co-person text-center to-animate">
                <h6><%= I18n.t(:'graphics.types.mark').strip %></h6>
                <img src="/graphics/img/green/bitcoin-cash-circle.svg" alt="Bitcoin Cash Logo" class="flag">
                <a href="/graphics/img/green/bitcoin-cash-circle.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-circle.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
      </section>
      <section id="fh5co-merchant" data-section="merchant">
        <div class="container">
          <div class="row">
            <div class="col-md-12 section-heading text-center">
              <h2 class="to-animate"><%= I18n.t(:'graphics.merchant.title').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext to-animate">
                  <h3><%= I18n.t(:'graphics.merchant.description').strip %></h3>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6">
              <div class="fh5co-person text-center to-animate">
                <img src="/graphics/img/green/bitcoin-cash-accepted-here-gt.svg" alt="Bitcoin Cash Accepted Here" class="">
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-gt.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-gt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-6">
              <div class="fh5co-person text-center to-animate">
                <img src="/graphics/img/green/bitcoin-cash-accepted-here-wt.svg" alt="Bitcoin Cash Accepted Here" class="">
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-wt.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-wt.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
            <div class="col-md-6">
              <div class="fh5co-person text-center to-animate">
                <img src="/graphics/img/green/bitcoin-cash-accepted-here-frame.svg" alt="Bitcoin Cash Accepted Here" class="">
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-frame.png" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.full').strip %></a>
                <a href="/graphics/img/green/bitcoin-cash-accepted-here-frame.svg" class="btn btn-secondary btn-block"><%= I18n.t(:'graphics.vector').strip %></a>
              </div>
            </div>
          </div>
        </div>
      </section>
      <section id="fh5co-identity" data-section="identity">
        <div class="container">
          <div class="row">
            <div class="col-md-6 section-heading text-center">
              <h2 class="to-animate"><%= I18n.t(:'graphics.identity.title').strip %></h2>
              <div class="row">
                <div class="col-md-8 col-md-offset-2 subtext to-animate">
                  <h3><%= I18n.t(:'graphics.identity.description').strip %></h3>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-6 to-animate">
              <div class="font">
                <h3><%= I18n.t(:'graphics.identity.font').strip %></h3>
                <h4 class="font-color-sample" style="font-weight:bold">ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890?!%</h4>
                <a href="https://fonts.google.com/specimen/Ubuntu?selection.family=Ubuntu:700" class="btn btn-primary" target="_blank"><%= I18n.t(:'graphics.identity.download').strip %></a>
              </div>
              <div class="font">
                <h3><%= I18n.t(:'graphics.identity.font_italic').strip %></h3>
                <h4 class="font-color-sample" style="font-style:italic">ABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890?!%</h4>
                <a href="https://fonts.google.com/specimen/Ubuntu?selection.family=Ubuntu:700i" class="btn btn-primary" target="_blank"><%= I18n.t(:'graphics.identity.download').strip %></a>
              </div>
            </div>
            <div class="col-md-6 to-animate">
              <div class="color row">
                <div class="col-xs-4 col-sm-3"><span class="box-color" style="background:#0AC18E"></span></div>
                <div class="col-xs-8 col-sm-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #0AC18E<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 247 / 148 / 29<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 0 / 50 / 100 / 0<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: 1375C
                </div>
              </div>
              <div class="color row">
                <div class="col-xs-4 col-sm-3"><span class="box-color" style="background:#4D4D4D"></span></div>
                <div class="col-xs-8 col-sm-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #4D4D4D<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 77 / 77 / 77<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 65 / 58 / 57 / 37<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: 11C
                </div>
              </div>
              <div class="color row">
                <div class="col-xs-4 col-sm-3"><span class="box-color" style="background:#FFFFFF"></span></div>
                <div class="col-xs-8 col-sm-9">
                  <%= I18n.t(:'graphics.identity.hex').strip %>: #FFFFFF<br>
                  <%= I18n.t(:'graphics.identity.rgb').strip %>: 255 / 255 / 255<br>
                  <%= I18n.t(:'graphics.identity.cmyk').strip %>: 0 / 0 / 0 / 0<br>
                  <%= I18n.t(:'graphics.identity.pantone').strip %>: paper
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <footer class="space--xs footer-1">
        <div class="container">
          <div class="row">
            <div class="col-sm-6">
              <span class="type--fine-print">&copy;<span class="update-year"></span> bitcoincash.org</span>
            </div>
            <div class="col-sm-6 text-right text-center-xs">
              <ul class="social-list list-inline list--hover">
                <li>
                  <a href="https://www.facebook.com/bitcoincashorg" target="_blank">
                    <i class="socicon socicon-facebook icon icon--xs"></i>
                  </a>
                </li>
                <li>
                  <a href="https://www.reddit.com/r/Bitcoincash/" target="_blank">
                    <i class="socicon socicon-reddit icon icon--xs"></i>
                  </a>
                </li>
                <li>
                  <a href="https://github.com/bitcoincashorg/website" target="_blank">
                    <i class="socicon socicon-github icon icon--xs"></i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </footer>
    </div>
    <a class="back-to-top inner-link" href="#start" data-scroll-class="100vh:active">
      <i class="stack-interface stack-up-open-big"></i>
    </a>
    <script src="/js/jquery.min.js"></script>
    <script src="/js/smooth-scroll.min.js"></script>
    <script src="/js/scripts.js"></script>
    <script src="/js/fontawesome.js"></script>
    <script src="/js/packs/solid.js"></script>
    <script src="/js/ticker.js?v=2"></script>
    <script src="/js/jquery.easing.1.3.js"></script>
    <script src="/js/jquery.stellar.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/jquery.waypoints.min.js"></script>
    <script src="/js/main.js"></script>
  </body>
</html>
