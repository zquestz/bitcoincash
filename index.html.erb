                      <div class="dropdown__content lang-menu">
                        <%-
                            I18n.available_locales.map do |locale|
                                [locale, I18n.t(:language, :locale => locale).strip]
                            end.sort_by { |pair| pair[1] }.each_slice(8) do |group|
                        -%>
                        <ul class="menu-vertical">
                          <%- group.each do |pair| -%>
                            <li>
                              <a href="/<%= pair[0].to_s.downcase %>/" class="external">
                                <%= pair[1] %>
                              </a>
                            </li>
                          <%- end -%>
                        </ul>
                      <%- end -%>
                    </div>
                  </div>
                </div>
              </div>
            </li>
